{
  "name": "rosscalimlim.me",
  "version": "1.0.0",
  "author": "Ross F. Calimlim <ross.calimlim@gmail.com>",
  "license": "MIT",
  "description": "Ross F. Calimlim's portfolio site.",
  "scripts": {
    "clean": "rimraf dist",
    "genPageSize": "bash scripts/page-size-data.sh",
    "genResumeRaw": "yarn run ts-node src/cv/main.ts",
    "compileLatex": "mkdir dist/files && lualatex --output-directory dist/files tex/resume.tex",
    "buildSite": "eleventy && yarn genPageSize && eleventy",
    "buildResume": "yarn genResumeRaw && yarn compileLatex",
    "start": "yarn clean && yarn buildSite && yarn buildResume && eleventy --serve"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@tsconfig/recommended": "^1.0.2",
    "@types/node": "^18.15.3",
    "clean-css": "^5.1.5",
    "date-fns": "^2.23.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^5.0.2"
  },
  "customDependencies": {
    "lualatex": "https://www.luatex.org/download.html"
  },
  "packageManager": "yarn@3.5.0"
}
